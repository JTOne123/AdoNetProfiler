{
  "title": "AdoNetProfiler",
  "version": "0.3.0",
  "description": "Profiler for ADO.NET with hooking DbConnection, DbCommand, DbDataReader, DbTransaction.",
  "authors": [ "t.takahari" ],
  "copyright": "Copyright t.takahari 2017",

  "frameworks": {
    "netstandard1.6": {
      "buildOptions": {
        "define": [ "COREFX" ]
      },
      "dependencies": {
        "System.Data.Common": "[4.1.0,)",
        "System.ComponentModel": "[4.0.1,)",
        "System.Linq": "[4.1.0,)",
        "System.Reflection": "[4.1.0,)",
        "System.Reflection.TypeExtensions": "[4.1.0,)",
        "System.Runtime.InteropServices": "[4.1.0,)",
        "System.Threading": "[4.0.11,)"
      }
    },
    "net46": {
      "frameworkAssemblies": {
        "System.Data": "4.0.0",
        "System.Xml": "4.0.0"
      }
    }
  },

  "buildOptions": {
    "xmlDoc": true
  },

  "packOptions": {
    "owners": [ "t.takahari" ],
    "requireLicenseAcceptance": false,
    "licenseUrl": "http://opensource.org/licenses/MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/ttakahari/AdoNetProfiler"
    },
    "projectUrl": "https://github.com/ttakahari/AdoNetProfiler",
    "tags": [ "ADO.NET" ]
  }
}
